    <style name="Animation" parent="android:Animation">  
    </style>
    <style name="Animation.Bottom">  
        <item name="android:windowEnterAnimation">@anim/slide_bottom_in</item>  
        <item name="android:windowExitAnimation">@anim/slide_bottom_out</item>  
    </style>
    <style name="Animation.Top">  
        <item name="android:windowEnterAnimation">@anim/slide_top_in</item>  
        <item name="android:windowExitAnimation">@anim/slide_top_out</item>  
    </style>
    <style name="AppBaseTheme" parent="@android:style/Theme.Holo.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="Button">
        <item name="android:background">@drawable/button</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/btn_text_size</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="Button.DialogNegative">
        <item name="android:textSize">@dimen/dialog_button_text_size</item>
    </style>
    <style name="Button.DialogPositive">
        <item name="android:textSize">@dimen/dialog_button_text_size</item>
    </style>
    <style name="Button.TitleBar">
        <item name="android:background">@drawable/title_bar_btn</item>
        <item name="android:textColor">@color/title_btn_text</item>
        <item name="android:textSize">@dimen/title_bar_btn_text_size</item>
    </style>
    <style name="Dialog" parent="@android:style/Theme.Dialog">
    	<item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
		<item name="android:windowFullscreen">true</item>
		<item name="android:windowNoTitle">true</item>
    </style>
    <style name="Dialog.Alert">
    	<item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="Dialog.Bottom">
    	<item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="EditText">
        <item name="android:background">@drawable/edit_text</item>
        <item name="android:textColor">@color/edit_text</item>
        <item name="android:textColorHint">@color/edit_text_hint</item>
        <item name="android:textSize">@dimen/edit_text_size</item>
        <item name="android:gravity">center_vertical|left</item>
    </style>
    <style name="EditText.Password">
        <item name="android:password">true</item>
        <item name="android:maxLength">24</item>
    </style>
    <style name="Text">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_normal_size</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="Text.DialogListItem">
        <item name="android:textSize">@dimen/dialog_list_item_text_size</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="Text.DialogMessage">
        <item name="android:textSize">@dimen/dialog_message_text_size</item>
        <item name="android:textColor">@color/gray_dark</item>
    </style>
    <style name="Text.DialogProgressMessage">
        <item name="android:textSize">@dimen/dialog_progress_message_text_size</item>
        <item name="android:textColor">@color/gray_dark</item>
    </style>
    <style name="Text.DialogTitle">
        <item name="android:textSize">@dimen/dialog_title_text_size</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="Text.PreviewContent">
        <item name="android:textColor">@color/gray_dark</item>
        <item name="android:textSize">@dimen/preview_content_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:lineSpacingMultiplier">1.5</item>
        <item name="android:minLines">2</item>
